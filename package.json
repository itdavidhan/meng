{
  "name": "meng",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "better-scroll": "^1.15.2",
    "core-js": "^3.4.3",
    "file-saver": "^2.0.2",
    "jszip": "^3.2.2",
    "ladash": "^1.2.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "view-design": "^4.0.2",
    "vue": "^2.6.10",
    "vue-baidu-map": "^0.21.22",
    "vue-lottie": "^0.2.1",
    "vue-router": "^3.1.3",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-var": "error",
      "no-console": "off",
      "no-debugger": "error",
      "object-shorthand": "error",
      "no-confusing-arrow": "error",
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
